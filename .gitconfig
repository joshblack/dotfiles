[user]
	name = Josh Black
	email = josh@josh.black

[color]
	ui = auto

[color "status"]
	untracked = blue

[include]
	path = .gitconfig.local

[credential]
	helper = osxkeychain

[merge]
	renamelimit = 2000
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
